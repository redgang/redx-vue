    "karma": "^0.13.22",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-static": "^2.0.0",
    "mocha": "^2.5.3",
    "marked": "^0.3.5",
    "nodemon": "^1.9.2",
    "phantomjs-polyfill": "0.0.2",
