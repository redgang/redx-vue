{
  "name": "redx-vue",
  "version": "0.0.6",
  "description": "基于vue、vue-router、vuex、webpack、postcss等实现移动端SPA解决方案",
  "main": "index.js",
  "engines": {
    "node": ">= 5.10.0",
    "npm": ">= 3.6.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint .",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "deploy": "better-npm-run deploy",
    "deploy:prod": "better-npm-run deploy:prod"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "vue",
    "vue-router",
    "vuex",
    "webpack",
    "postcss",
    "es6"
  ],
  "author": "Red钢 <redx123@126.com> (https://github.com/redgang)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redgang/redx-vue/issues"
  },
  "homepage": "https://github.com/redgang/redx-vue",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "0.0.9",
    "co-request": "1.0.0",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^3.0.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.4.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-vue": "0.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.28.0",
